#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Seminario-Grupo-15/Backend/node_modules/ps-tree/bin/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/Backend/node_modules/ps-tree/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/Backend/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Seminario-Grupo-15/Backend/node_modules/ps-tree/bin/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/Backend/node_modules/ps-tree/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/Backend/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Seminario-Grupo-15/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ps-tree/bin/ps-tree.js" "$@"
else
  exec node  "$basedir/../ps-tree/bin/ps-tree.js" "$@"
fi
